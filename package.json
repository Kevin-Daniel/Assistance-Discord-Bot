{
	"name": "inviteblue",
	"version": "0.1.0",
	"description": "",
	"main": "./bin/bot.js",
	"scripts": {
		"start": "cross-env NODE_ENV=dev node scripts/dev-run.js",
		"debug": "cross-env NODE_ENV=dev node scripts/dev-run.js debug",
		"build": "tsc",
		"lint": "tslint --project tsconfig.json --config tslint.json --format stylish",
		"format": "prettier --write \"src/**/*.ts\" \"docs/**/*.md\" \"README.md\" \"CHANGELOG.md\"",
		"release": "npm run build && npm run docs && npm run format && standard-version",
		"i18n": "node scripts/check-i18n.js",
		"nxv": "node ./scripts/npm-exact-versions.js",
		"docs": "node scripts/generate-docs.js"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run nxv && pretty-quick --staged"
		}
	},
	
	"author": "Ceed",
	"license": "ISC",
	
	"homepage": "https://inviteblue.eu/",
	"dependencies": {
		"@sentry/node": "6.13.3",
		"@types/offscreencanvas": "^2019.6.4",
		"@types/resemblejs": "^3.2.1",
		"amqplib": "0.8.0",
		"axios": "^0.23.0",
		"bufferutil": "4.0.5",
		"canvas": "2.8.0",
		"chalk": "4.1.2",
		"chart.js": "^3.5.1",
		"chartjs-node-canvas": "^4.0.1",
		"chartjs-plugin-datalabels": "2.0.0",
		"eris": "^0.16.1",
		"eris-lavalink": "github:SideProjectGuys/eris-lavalink#8e3156a968033c1f037b5424dd46aacb8af76436",
		"erlpack": "github:abalabahaha/erlpack",
		"eventemitter3": "4.0.7",
		"i18n": "0.13.3",
		"iheart": "3.1.1",
		"moment": "2.29.1",
		"mysql2": "2.3.0",
		"reflect-metadata": "0.1.13",
		"superagent": "^7.1.1",
		"uws": "200.0.0",
		"xmldoc": "1.1.2",
		"ytdl-core": "4.9.1",
		"zlib-sync": "0.1.7"
	},
	"devDependencies": {
		"@sentry/cli": "1.69.1",
		"@types/amqplib": "0.8.2",
		"@types/chart.js": "2.9.34",
		"@types/i18n": "0.13.1",
		"@types/mysql2": "github:types/mysql2",
		"@types/node": "16.11.0",
		"@types/superagent": "^4.1.15",
		"@types/ws": "^8.2.0",
		"@types/xmldoc": "1.1.6",
		"cross-env": "7.0.3",
		"docsify": "4.12.1",
		"docsify-cli": "4.4.3",
		"fs-extra": "10.0.0",
		"husky": "7.0.2",
		"prettier": "2.4.1",
		"pretty-quick": "3.1.1",
		"standard-version": "9.3.1",
		"tslint": "6.1.2",
		"tslint-config-prettier": "1.18.0",
		"typescript": "4.4.4",
		"typestrict": "1.0.2"
	},
	"engines": {
		"node": ">=14.18.1"
	}
}
